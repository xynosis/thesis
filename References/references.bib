	author = {DeVito, Michael A. and Birnholtz, Jeremy and Hancock, Jeffery T. and French, Megan and Liu, Sunny},
	year = {2018},
	keywords = {algorithm awareness, algorithmic curation, algorithms, folk theories, grounded theory, social feeds, social media, theory formation, user understandings},
	pages = {120:1--120:12},
}

@inproceedings{marshall_accountability_2018,
	address = {New York, NY, USA},
	series = {{CHI} '18},
	title = {Accountability {Work}: {Examining} the {Values}, {Technologies} and {Work} {Practices} {That} {Facilitate} {Transparency} in {Charities}},
