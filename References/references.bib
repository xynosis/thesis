@article{gaver_ambiguity_2003,
	title = {Ambiguity as a {Resource} for {Design}},
	abstract = {Ambiguity is usually considered anathema in Human Computer Interaction. We argue, in contrast, that it is a resource for design that can be used to encourage close personal engagement with systems. We illustrate this with examples from contemporary arts and design practice, and distinguish three broad classes of ambiguity according to where uncertainty is located in the interpretative relationship linking person and artefact. Ambiguity of information finds its source in the artefact itself, ambiguity of context in the sociocultural discourses that are used to interpret it, and ambiguity of relationship in the interpretative and evaluative stance of the individual. For each of these categories, we describe tactics for emphasising ambiguity that may help designers and other practitioners understand and craft its use.},
	language = {en},
	number = {5},
	journal = {NEW HORIZONS},
	author = {Gaver, William W and Beaver, Jacob and Benford, Steve},
	year = {2003},
	pages = {8},
}

@incollection{hutchison_moving_2006,
	address = {Berlin, Heidelberg},
	title = {Moving on from {Weiser}’s {Vision} of {Calm} {Computing}: {Engaging} {UbiComp} {Experiences}},
	volume = {4206},
	isbn = {978-3-540-39634-5 978-3-540-39635-2},
	shorttitle = {Moving on from {Weiser}’s {Vision} of {Calm} {Computing}},
	url = {http://link.springer.com/10.1007/11853565_24},
	abstract = {A motivation behind much UbiComp research has been to make our lives convenient, comfortable and informed, following in the footsteps of Weiser’s calm computing vision. Three themes that have dominated are context awareness, ambient intelligence and monitoring/tracking. While these avenues of research have been fruitful their accomplishments do not match up to anything like Weiser’s world. This paper discusses why this is so and argues that is time for a change of direction in the field. An alternative agenda is outlined that focuses on engaging rather than calming people. Humans are very resourceful at exploiting their environments and extending their capabilities using existing strategies and tools. I describe how pervasive technologies can be added to the mix, outlining three areas of practice where there is much potential for professionals and laypeople alike to combine, adapt and use them in creative and constructive ways.},
	language = {en},
