	author = {Jun, Joohee and Jung, Myeongul and Kim, So-Yeon and Kim, Kwanguk (Kenny)},
	year = {2018},
	keywords = {emotion, full-body ownership illusion, motion-capture, virtual embodiment},
	pages = {601:1--601:11},
}

