	year = {2018},
	keywords = {civic engagement, civic technologies, digital civics, public services, qualitative research, service directories},
	pages = {442:1--442:13},
}

@inproceedings{taylor_strategies_2018,
	address = {New York, NY, USA},
	series = {{CHI} '18},
	title = {Strategies for {Engaging} {Communities} in {Creating} {Physical} {Civic} {Technologies}},
	isbn = {978-1-4503-5620-6},
