	year = {2018},
	keywords = {big data, boundary objects, civic data, data privacy, data security, data sharing, design games, dynamic consent, ethnographic interviews, family, family design games, family research, healthcare, personal data, social care, ubicomp, user-centered design},
	pages = {136:1--136:13},
}

@inproceedings{dow_between_2018,
	address = {New York, NY, USA},
	series = {{CHI} '18},
	title = {Between {Grassroots} and the {Hierarchy}: {Lessons} {Learned} from the {Design} of a {Public} {Services} {Directory}},
