@inproceedings{morrissey_im_2016,
	address = {New York, NY, USA},
	series = {{DIS} '16},
	title = {'{I}'m a {Rambler}, {I}'m a {Gambler}, {I}'m a {Long} {Way} from {Home}': {The} {Place} of {Props}, {Music}, and {Design} in {Dementia} {Care}},
