@inproceedings{bellini_mechanisms_2020,
	address = {Honolulu, HI, USA},
	series = {{CHI} '20},
	title = {Mechanisms of {Moral} {Responsibility}: {Rethinking} {Technologies} for {Domestic} {Violence} {Prevention} {Work}},
