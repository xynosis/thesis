@article{baba_end_2005,
	title = {To {The} {End} {Of} {Theory}-{Practice} ‘{Apartheid}’: {Encountering} {The} {World}},
	volume = {2005},
	issn = {1559-8918},
	shorttitle = {To {The} {End} {Of} {Theory}-{Practice} ‘{Apartheid}’},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1559-8918.2005.tb00023.x},
	doi = {10.1111/j.1559-8918.2005.tb00023.x},
	abstract = {A historical and comparative examination of ethnographic practice in sixteen nations around the globe reveals that theory-practice relations in anthropology and ethnography (A/E) have been shaped and re-shaped over time and space by complex contextual influences. This paper explores the evolution of theorypractice relationships in A/E over various regions of the world, tracing the beginning of a theory-practice ‘split’ from its origin under British colonialism, to its reappearance and institutionalization in post-World War II America, and postulating its absence in the ‘Second and Third Worlds’. Global practice in ethnography now appears to be converging toward a re-integration of theory and application across multiple disciplines and professions (a ‘hybrid’ approach), as ethnographers work to address urgent and poorly understood problems that are not well theorized.},
	language = {en},
	number = {1},
	urldate = {2022-02-03},
	journal = {Ethnographic Praxis in Industry Conference Proceedings},
	author = {Baba, Marietta},
	year = {2005},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1559-8918.2005.tb00023.x},
	pages = {205--217},
}

@book{puddephatt_ethnographies_2009,
	address = {London; New York},
	title = {Ethnographies revisited: constructing theory in the field},
	isbn = {978-0-415-45220-5 978-0-415-45221-2 978-0-203-87650-3},
	shorttitle = {Ethnographies revisited},
	abstract = {"Ethnographies Revisited provides first-hand accounts of how leading qualitative researchers crafted key theoretical concepts found in their major book-length ethnographies. Great ethnographic research lies not in the rigid execution of prescribed methodological procedures, but on the unrelenting cultivation of theoretical ideas. These contributors focus squarely on this neglected topic, providing reflexive accounts of how research decisions were made in light of emerging theoretical questions. The continuous generation of creative concepts is arguably the most important skill in developing powerful results in field research, since the originality of the ideas produced is how the study is ultimately judged. Yet, this topic is often taken for granted, treated rigidly and artificially, or is entirely absent from existing qualitative research manuals. In contrast, this volume offers candid insights of how leading ethnographers generated their initial questions, chose their research sites, made theoretical and methodological adjustments, and oriented their research to maximize the conceptual payoff, leading to such successful research contributions. This provides a fresh approach to the topic of qualitative research, by linking practical decisions in the field to the dynamic features of theory in the making, told through the first-hand experiences of some of the best ethnographers in our field."--Back cover.},
	language = {English},
	publisher = {Routledge},
	author = {Puddephatt, Antony J and Shaffir, William and Kleinknecht, Steven W},
	year = {2009},
	note = {OCLC: 291453868},
}

