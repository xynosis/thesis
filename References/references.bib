@inproceedings{vlachokyriakos_infrastructuring_2018,
	address = {New York, NY, USA},
	series = {{CHI} '18},
	title = {Infrastructuring the {Solidarity} {Economy}: {Unpacking} {Strategies} and {Tactics} in {Designing} {Social} {Innovation}},
