	keywords = {Coalitions, Feminism, Matters of Care, Rural Women, Social Innovation},
	pages = {17--20},
}

@inproceedings{palacin_design_2020,
	address = {New York, NY, USA},
	series = {{PDC} '20},
	title = {The {Design} of {Pseudo}-{Participation}},
	isbn = {978-1-4503-7606-8},
	url = {https://doi.org/10.1145/3384772.3385141},
	doi = {10.1145/3384772.3385141},
	abstract = {Participation is key to building an equitable, realistic and democratic future. Yet a lack of agency in decision making and agenda-setting is a growing phenomenon in the design of digital public services. We call this pseudo-participation by and in design. The configuration of digital artifacts and/or processes can provide an illusion of participation but lack supportive processes and affordances to allow meaningful participation to happen. This exploratory paper examines the realm of pseudo-participation in the design of public digital services through two concepts: 1) pseudo-participation by design, digital interfaces, and tools that provide the illusion of participation to the people, 2) pseudo-participation in design, processes in which those affected by the design decisions are marginalized and not given any agency. We contribute to the re-imagination of participatory design in modern societies where the role of politics has become ubiquitous and is yet to be critically scrutinized by designers.},
	urldate = {2021-11-05},
	booktitle = {Proceedings of the 16th {Participatory} {Design} {Conference} 2020 - {Participation}(s) {Otherwise} - {Volume} 2},
	publisher = {Association for Computing Machinery},
	author = {Palacin, Victoria and Nelimarkka, Matti and Reynolds-Cuéllar, Pedro and Becker, Christoph},
	month = jun,
	year = {2020},
	keywords = {digital services, pseudo-participation by design, pseudo-participation in design, technocratic clientelism, user configuration},
	pages = {40--44},
}

@inproceedings{gautam_p_2020,
	address = {New York, NY, USA},
	series = {{PDC} '20},
	title = {p for political: {Participation} {Without} {Agency} {Is} {Not} {Enough}},
	isbn = {978-1-4503-7606-8},
	shorttitle = {p for political},
	url = {https://doi.org/10.1145/3384772.3385142},
	doi = {10.1145/3384772.3385142},
	abstract = {Participatory Design’s vision of democratic participation assumes participants’ feelings of agency in envisioning a collective future. But this assumption may be leaky when dealing with vulnerable populations. We reflect on the results of a series of activities aimed at supporting agentic-future-envisionment with a group of sex-trafficking survivors in Nepal. We observed a growing sense among the survivors that they could play a role in bringing about change in their families. They also became aware of how they could interact with available institutional resources. Reflecting on the observations, we argue that building participant agency on the small and personal interactions is necessary before demanding larger Political participation. In particular, a value of PD, especially for vulnerable populations, can lie in the process itself if it helps participants position themselves as actors in the larger world.},
	urldate = {2021-11-05},
	booktitle = {Proceedings of the 16th {Participatory} {Design} {Conference} 2020 - {Participation}(s) {Otherwise} - {Volume} 2},
