}

@inproceedings{fiesler_we_2018,
	address = {New York, NY, USA},
	series = {{CHI} '18},
	title = {"{We} {Are} the {Product}": {Public} {Reactions} to {Online} {Data} {Sharing} and {Privacy} {Controversies} in the {Media}},
