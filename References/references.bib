	pages = {604:1--604:13},
}

@inproceedings{schlesinger_lets_2018,
	address = {New York, NY, USA},
	series = {{CHI} '18},
	title = {Let's {Talk} {About} {Race}: {Identity}, {Chatbots}, and {AI}},
	isbn = {978-1-4503-5620-6},
