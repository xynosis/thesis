@inproceedings{skirpan_more_2018,
	address = {New York, NY, USA},
	series = {{CHI} '18},
	title = {More {Than} a {Show}: {Using} {Personalized} {Immersive} {Theater} to {Educate} and {Engage} the {Public} in {Technology} {Ethics}},
