	pages = {alt02:1--alt02:8},
}

@inproceedings{strohmayer_chiversity:_2018,
	address = {New York, NY, USA},
	series = {{CHI} {EA} '18},
	title = {\#{CHIversity}: {Implications} for {Equality}, {Diversity}, and {Inclusion} {Campaigns}},
