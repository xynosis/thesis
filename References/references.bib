	pages = {45--49},
}

@inproceedings{e_kozak_sticks_2020,
	address = {New York, NY, USA},
	series = {{PDC} '20},
	title = {Sticks, {Ropes}, {Land}: {Confronting} {Colonial} {Practices} in {Public} {Space} {Design}},
