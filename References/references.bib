	booktitle = {Proceedings of the 2019 {CHI} {Conference} on {Human} {Factors} in {Computing} {Systems}},
	publisher = {ACM},
	author = {Freeman, Guo and Bardzell, Jeffrey and Bardzell, Shaowen and Liu, Szu-Yu (Cyn) and Lu, Xi and Cao, Diandian},
